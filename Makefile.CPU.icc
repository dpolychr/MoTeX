MF=	Makefile.CPU.icc

CC=	icc

CFLAGS=	-g -ipo -D_USE_CPU

LFLAGS=	-limf -lz -lm -ldatrie   

EXE=	motexCPU

SRC= 	motex.c functions.c

HD=     motexdefs.h triedefs.h trie.h alpha-map.h typedefs.h

#
# No need to edit below this line
#

.SUFFIXES:
.SUFFIXES: .c .o

OBJ=	$(SRC:.c=.o)

.c.o:
	$(CC) $(CFLAGS) -c $<

all:	$(EXE) 

$(EXE):	$(OBJ)
	$(CC) $(CFLAGS) -o $@ $(OBJ) $(LFLAGS)

$(OBJ):	$(MF) $(HD)

clean:
	rm -f $(OBJ) $(EXE)
